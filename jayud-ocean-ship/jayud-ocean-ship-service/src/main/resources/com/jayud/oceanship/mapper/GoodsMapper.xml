        <result column="business_type" property="businessType" />
        <result column="order_no" property="orderNo" />
        <result column="label" property="label" />
        <result column="name" property="name" />
        <result column="plate_amount" property="plateAmount" />
        <result column="plate_unit" property="plateUnit" />
        <result column="bulk_cargo_amount" property="bulkCargoAmount" />
        <result column="bulk_cargo_unit" property="bulkCargoUnit" />
        <result column="size" property="size" />
        <result column="total_weight" property="totalWeight" />
        <result column="volume" property="volume" />
        <result column="create_time" property="createTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, business_id, business_type, order_no, label, name, plate_amount, plate_unit, bulk_cargo_amount, bulk_cargo_unit, size, total_weight, volume, create_time
    </sql>

</mapper>
