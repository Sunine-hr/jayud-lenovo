<mapper namespace="com.jayud.oceanship.mapper.TermsMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.jayud.oceanship.model.po.Terms">
        <id column="id" property="id" />
