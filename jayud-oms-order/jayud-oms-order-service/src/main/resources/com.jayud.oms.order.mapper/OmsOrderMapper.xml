    <delete id="phyDelById">
        DELETE FROM  oms_order WHERE id = ${id}
    </delete>

    `<update id="logicDel">
        UPDATE oms_order SET is_deleted = 1 ,update_by = #{username}, update_time = now()
        WHERE id = ${id} AND is_deleted = 0
    </update>

